include ../product-toggl/product-toggl

mixin product-filter(data,check)
	-data = data || {}
	-check = check || []
	+b.product-filter
		+e.title.js-filter-title= data.title
		each el in check
			+e.container
				+e.option.js-filter-option= el.opt
					+e.tooltip
					+e.value
						+e.check
							each item in el.check
								+product-toggl(item)
